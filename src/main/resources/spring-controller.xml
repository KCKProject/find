<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.1.xsd">

	<mvc:view-controller path="/main" view-name="main"/>
	
	<mvc:view-controller path="/" view-name="main"/>
	
	
	<bean id="AdminLoginController" class="com.green.KCK_find.controller.AdminLoginController">
		<property name="adminAuthService" ref="adminAuthSvc" />
	</bean>
	
	<bean id="loginController" class="spring.controller.LoginController"/>
	<bean id="sighUpController" class="spring.controller.SighUpController"/>
	<bean id="lostBoardController" class="spring.controller.LostBoardListController"/>

</beans>
